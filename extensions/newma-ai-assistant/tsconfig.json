{
	"compilerOptions": {
		"outDir": "out",
		"rootDir": "src",
		"lib": ["ES2020", "DOM", "WebWorker"],
		"module": "commonjs",
		"target": "ES2020",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "out"]
}
